page {
  background-color: #f5f5f5;
}

.charts-wrap {
  background-color: #fff;
  margin-top: 20rpx;
  padding: 20rpx;
}

.charts-wrap-head {
  font-size: 30rpx;
  color: #333;
  padding: 20rpx 0;
}

.canvas {
  width: 100%;
  height: 200px;
}

.no-charts {
  height: 200rpx;
  line-height: 200rpx;
  text-align: center;
  font-size: 30rpx;
  color: #666;
}

.charts-search {
  font-size: 28rpx;
  color: #333;
  display: flex;
  margin-bottom: 15rpx;
}

.charts-search .charts-search-item {
  flex: 1;
  padding: 0 10rpx;
  width: 334rpx;
}
.charts-search .charts-search-item.auto{
  width: auto;
}
.search-item-lable {
  margin-bottom: 10rpx;
}

.search-input {
  padding: 0 10rpx;
  border: 1px solid rgba(0, 0, 0, 0.1);
  outline: 0;
  height: 60rpx;
  line-height: 60rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #666;
}
.input-holder{
  color: #666;
}
.search-btn{
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  width: 120rpx;
  text-align: center;
  background: #48CFCF;
  color: #fff;
  font-size: 26rpx;
  margin: 52rpx 10rpx 0;
  border-radius: 6rpx;
}
